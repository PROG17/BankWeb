@model List<Account>

@{
    ViewData["Title"] = "Success!";
}

<div class="row mt-50">

    @for (int i = 0; i < Model.Count; i++)
    {
        <div class="col-lg-12 text-center">
            <h2><b>Accountnumber:</b> @Model[i].AccountId</h2>
            <h3>New balance is: <span class="text-primary">@Model[i].Balance</span></h3>
        </div>
    }

</div>
